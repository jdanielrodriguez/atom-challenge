{
   "hosting": {
      "public": "/firebase/config/hosting/browser",
      "ignore": [
         "firebase.json",
         "**/.*",
         "**/node_modules/**"
      ]
   },
   "firestore": {
      "rules": "firestore.rules"
   },
   "functions": {
      "source": "functions",
      "runtime": "nodejs18"
   },
   "emulators": {
      "ui": {
         "enabled": true,
         "host": "0.0.0.0",
         "port": 4000
      },
      "firestore": {
         "host": "0.0.0.0",
         "port": 8080
      },
      "functions": {
         "host": "0.0.0.0",
         "port": 5001
      },
      "auth": {
         "host": "0.0.0.0",
         "port": 9099
      },
      "database": {
         "host": "0.0.0.0",
         "port": 9000,
         "rules": "database.rules.json"
      }
   }
}